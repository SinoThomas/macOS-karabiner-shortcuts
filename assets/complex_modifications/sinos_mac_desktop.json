{
  "title": "Sino's mac utilities with fn + *",
  "maintainers": ["sino"],
  "homepage": "https://github.com/SinoThomas/macOS-karabiner-shortcuts",
  "import_url": "karabiner://karabiner/assets/complex_modifications/import?url=https://raw.githubusercontent.com/SinoThomas/macOS-karabiner-shortcuts/master/sinos_mac_utilities.json",
  "rules": [
      {
          "description": "Fn + Z              Ctrl 1   Desktop 1",
          "manipulators": [
              {
                  "type": "basic",
                  "from": { "key_code": "z", "modifiers": { "mandatory": ["fn"] } },
                  "to": [ { "key_code": "1", "modifiers": ["left_control"] } ]
              }
          ]
      },
      {
          "description": "Fn + X              Ctrl 2   Desktop 2",
          "manipulators": [
              {
                  "type": "basic",
                  "from": { "key_code": "x", "modifiers": { "mandatory": ["fn"] } },
                  "to": [ { "key_code": "2", "modifiers": ["left_control"] } ]
              }
          ]
      },
      {
          "description": "Fn + C              Ctrl 3   Desktop 3",
          "manipulators": [
              {
                  "type": "basic",
                  "from": { "key_code": "c", "modifiers": { "mandatory": ["fn"] } },
                  "to": [ { "key_code": "3", "modifiers": ["left_control"] } ]
              }
          ]
      }
  ]
}
