  "rules": [
    {
      "description": "Fn + Space         Mouse Right Click",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "pointing_button": "button2"
            }
          ]
        }
      ]
    },

    {
